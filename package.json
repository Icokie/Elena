{
  "name": "elena",
  "version": "1.0.0",
  "main": "index.js",
  "author": "icokie <icokie@yandex.ru>",
  "license": "MIT",
  "scripts": {
    "prestart": "yarn clean && yarn cp:assets",
    "start": "yarn dev:content",
    "clean": "rm -rf dist",
    "dev:content": "parcel src/contentScript/dev-index.html",
    "dev:popUpApp": "parcel src/popUpApp/index.html",
    "build:content": "parcel build src/contentScript/index.tsx -d dist/contentScript",
    "build:popUpApp": "parcel build src/popUpApp/index.html -d dist/popUpApp --public-url ./",
    "build": "yarn clean && yarn cp:assets && yarn build:content && yarn build:popUpApp",
    "cp:assets": "yarn cp:static && yarn cp:manifest",
    "cp:manifest": "cp src/manifest.json dist",
    "cp:static": "cp -r src/static dist",
    "lint": "eslint 'src/**/*.{ts,tsx}' --quiet --fix",
    "format": "prettier 'src/**/*.{ts,tsx,json,less}' --write"
  },
  "dependencies": {
    "dexie": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hanger": "^2.2.1",
    "tailwindcss": "^1.4.4",
    "cogo-toast": "^4.2.3"
  },
  "devDependencies": {
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "babel-types": "^6.26.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-standard": "^4.0.1",
    "less": "^3.11.1",
    "parcel-bundler": "^1.12.4",
    "postcss": "^7.0.28",
    "postcss-remove-unused-css": "^1.0.4",
    "prettier": "^2.0.5",
    "typescript": "^3.8.3"
  }
}

{
  "manifest_version": 2,

  "name": "Elena",
  "description": "Words/text trainer",
  "version": "0.0.1",

  "icons": {
    "16": "images/elena-logo.png",
    "48": "images/elena-logo.png",
    "128": "images/elena-logo.png"
  },
  "content_scripts": [
    {
      "js": ["./contentScript/index.js"],
      "css": ["./contentScript/index.css"],
      "matches": ["<all_urls>"]
    }
  ],
  "browser_action": {
    "default_popup": "popUpApp/index.html",
    "default_title": "Elena Ext"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "permissions": ["storage", "activeTab"]
}
